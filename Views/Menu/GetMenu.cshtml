@model IEnumerable<GoldenPet.Models.tb_Menu>

<div class="navbar-nav mr-auto py-0">
    @foreach (var i in Model)
    {
        if (i.name != "Home")
        {
            <a href="@Url.Action(i.name, "Home")" class="nav-item nav-link active">@i.name</a>
        }
        else
        {
            <a href="/Home" class="nav-item nav-link active">@i.name</a>
        }

    }

</div>

